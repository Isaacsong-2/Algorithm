   "source": [
    "n, k = map(int, input().split())\n",
    "\n",
    "ans = 0\n",
    "\n",
    "while bin(n).count('1') > k:\n",
    "    idx = bin(n)[::-1].index('1')\n",
    "    ans += 2**idx\n",
    "    n += 2**idx\n",
    "\n",
    "print(ans)"
   ]
  }
