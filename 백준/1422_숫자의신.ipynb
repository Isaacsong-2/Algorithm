    "from functools import cmp_to_key\n",
    "temp = max(num)\n",
    "for _ in range(n-k):\n",
    "    num.append(temp)\n",
    "num = sorted(num, key= cmp_to_key(lambda x,y: int(str(y)+str(x))-int(str(x)+str(y))))\n",
    "\n",
    "print(*num, sep=\"\")"
