   ],
   "source": [
    "dp = [0] * (n+1)\n",
    "cur = 0\n",
    "for i in range(n):\n",
    "    cur = max(cur, dp[i])\n",
    "    t, p = li[i]\n",
    "    if i+t<=n:\n",
    "        dp[i+t] = max(dp[i+t],cur+p)\n",
    "print(max(dp))"
   ]
  }
