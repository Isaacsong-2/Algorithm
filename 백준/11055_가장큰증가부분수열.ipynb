     ]
    }
   ],
   "source": [
    "dp = [0] * n\n",
    "dp[0] = li[0]\n",
    "for i in range(1,n):\n",
    "    for j in range(i):\n",
    "        if li[j] < li[i]:\n",
    "            dp[i] = max(dp[i],dp[j]+ li[i])\n",
    "    if j == i-1 and dp[i] == 0:\n",
    "        dp[i] = li[i]\n",
    "print(max(dp))"
   ]
  }
