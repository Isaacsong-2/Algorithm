    "t = int(input())\n",
    "for _ in range(t):\n",
    "    n, m = map(int,input().split())\n",
    "    dp = [[0] * (m+1) for _ in range(n+1)]\n",
    "    for i in range(m):\n",
    "        dp[0][i] = 1\n",
    "    for i in range(1,n+1):\n",
    "        for j in range(1,m+1):\n",
    "            dp[i][j] = dp[i][j-1] + dp[i-1][j-1]\n",
    "    print(dp[n][m])"
   ]
  }
