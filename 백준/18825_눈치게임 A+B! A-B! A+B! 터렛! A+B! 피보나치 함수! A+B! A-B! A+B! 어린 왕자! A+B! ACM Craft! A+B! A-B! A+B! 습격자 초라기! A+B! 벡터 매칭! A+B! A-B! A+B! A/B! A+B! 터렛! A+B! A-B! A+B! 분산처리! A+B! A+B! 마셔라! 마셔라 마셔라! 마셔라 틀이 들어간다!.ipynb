    "n = 521\n",
    "visit = [0] * (n+3)\n",
    "prime = []\n",
    "for i in range(2, n+3):\n",
    "    if visit[i] == 0:\n",
    "        for j in range(0,n+3,i):\n",
    "            visit[j] = 1\n",
    "        prime.append(i)\n",
    "k = 0\n",
    "for pr in prime:\n",
    "    k += 1\n",
    "    if (n+2)%pr == 0:\n",
    "        p = pr\n",
    "        break\n",
    "print(999+k)"
