    "import re\n",
    "t = int(input())\n",
    "for _ in range(t):\n",
    "    strin = input().replace('\\n','')\n",
    "    temp = re.compile('(100+1+|01)+')\n",
    "    ans = temp.fullmatch(strin)\n",
    "    print('YES') if ans else print('NO')"
   ]
  }
