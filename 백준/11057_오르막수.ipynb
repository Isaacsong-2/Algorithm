   "source": [
    "n = int(input())\n",
    "dp = [[1]*10]+[[0]*10 for _ in range(n-1)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1464\n"
     ]
    }
   ],
   "source": [
    "for i in range(1,n):\n",
    "    for j in range(10):\n",
    "        for k in range(j,10):\n",
    "            dp[i][j] += dp[i-1][k]\n",
    "print(sum(dp[n-1])%10007)"
   ]
  }
