      "100\n"
     ]
    }
   ],
   "source": [
    "sol = 0\n",
    "while(len(arr)-1):\n",
    "    num1 = heapq.heappop(arr)\n",
    "    num2 = heapq.heappop(arr)\n",
    "    sol += num1+num2\n",
    "    heapq.heappush(arr, num1+num2)\n",
    "print(sol)"
   ]
  }
