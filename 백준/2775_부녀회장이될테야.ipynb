    "t = int(input())\n",
    "dp = [[0] * 15 for _ in range(15)]\n",
    "for i in range(1,15):\n",
    "    dp[0][i] = i\n",
    "for i in range(1,15):\n",
    "    for j in range(1,15):\n",
    "        dp[i][j] = sum(dp[i-1][:j+1])\n",
    "for _ in range(t):\n",
    "    k = int(input())\n",
    "    n = int(input())\n",
    "    print(dp[k][n])"
