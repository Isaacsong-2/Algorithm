   "source": [
    "while(True):\n",
    "    n = int(input())\n",
    "    if n == 0:\n",
    "        break\n",
    "\n",
    "    dp = [[0] * n for _ in range(2*n)]\n",
    "    dp[0][0] = 1\n",
    "\n",
    "    for i in range(1,2*n):\n",
    "        for j in range(i//2+1):\n",
    "            dp[i][j] = dp[i-1][j] + dp[i-1][j-1]\n",
    "    print(dp[-1][-1]) if n != 1 else print(1)\n"
   ]
  }
