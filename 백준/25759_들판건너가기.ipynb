   "source": [
    "n = int(input())\n",
    "li = list(map(int,input().split()))\n",
    "dp = [0] * n\n",
    "beauty = [0] * 101\n",
    "for i in range(1,n):\n",
    "    beauty[li[i]] = i\n",
    "    for j in range(1, 101):\n",
    "        idx = beauty[j]\n",
    "        dp[i] = max(dp[i], dp[idx] + (li[i] - li[idx])**2)\n",
    "print(dp[n-1])"
   ]
  }
 ],
